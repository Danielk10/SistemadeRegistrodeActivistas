<?xml version="1.0"?>
<openerp>
<data>

    <record model="ir.ui.view" id="view_activista_form">
        <field name="name">activista.form</field>
        <field name="model">activista</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Activista">
            <group string="'Registro del Activista'" col="4" colspan="2">
                <field name="nombres" select="1"/>
                <field name="apellidos" select="2"/>
                <field name="cedula" select="0"/>
                <field name="correo" select="0"/>
                <field name="telefono_ids" select="0" widget="one2many">
                    <tree>
                        <field name="telefono"/>
                    </tree>
                </field>
                <field name="proyectos_ids" select="0"  widget="one2many">
                    <tree>
                        <field name="proyecto"/>
                    </tree>
                </field>
                <field name="repositorio_tipo" select="0"/>
                <field name="repositorio_url" select="0"/>
                <separator string="'Dirección de Domicilio'" colspan="4"/>
                <field name="dom_estado_id" select="0"/>
                <field name="dom_municipio_id" domain="[('estado_id','=',[dom_estado_id])]" select="0"/>
                <field name="dom_parroquia_id" domain="[('municipio_id','=',[dom_municipio_id])]" select="0"/>
                <separator string="'Dirección del Lugar de Trabajo'" colspan="4"/>
                <field name="trab_estado_id" select="0"/>
                <field name="trab_municipio_id" domain="[('estado_id','=',[trab_estado_id])]" select="0"/>
                <field name="trab_parroquia_id" domain="[('municipio_id','=',[trab_municipio_id])]" select="0"/>


            </group>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="view_activista_tree">
        <field name="name">activista.tree</field>
        <field name="model">activista</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Activista">
                <field name="nombres"/>
                <field name="apellidos"/>
                <field name="cedula"/>
                <field name="telefono_ids"/>
                <field name="correo"/>
                <field name="dom_estado_id"/>
                <field name="dom_municipio_id"/>
                <field name="dom_parroquia_id"/>
                <field name="proyectos_ids"/>
                <field name="trab_estado_id"/>
                <field name="trab_municipio_id"/>
                <field name="trab_parroquia_id"/>
                <field name="repositorio_tipo"/>
                <field name="repositorio_url"/>

            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="action_activista">
        <field name="name">activista</field>
        <field name="res_model">activista</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>

        
    <record model="ir.ui.view" id="view_telefonoactivista_form">
        <field name="name">telefonoactivista.form</field>
        <field name="model">telefonoactivista</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="TelefonoActivista">
            <group col="4" colspan="2">
                <field name="telefono" select="1"/>

            </group>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="view_telefonoactivista_tree">
        <field name="name">telefonoactivista.tree</field>
        <field name="model">telefonoactivista</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="TelefonoActivista">
                <field name="telefono"/>
                <field name="activista_id"/>

            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="action_telefonoactivista">
        <field name="name">Telefono Del Activista</field>
        <field name="res_model">telefonoactivista</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>

        
    <record model="ir.ui.view" id="view_estado_form">
        <field name="name">estado.form</field>
        <field name="model">estado</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Estado">
            <group col="4" colspan="2">
                <field name="codigo" select="1"/>
                <field name="estado" select="2"/>

            </group>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="view_estado_tree">
        <field name="name">estado.tree</field>
        <field name="model">estado</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Estado">
                <field name="codigo"/>
                <field name="estado"/>

            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="action_estado">
        <field name="name">estado</field>
        <field name="res_model">estado</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>

        
    <record model="ir.ui.view" id="view_municipio_form">
        <field name="name">municipio.form</field>
        <field name="model">municipio</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="municipio">
            <group col="4" colspan="2">
                <field name="codigo" select="1"/>
                <field name="municipio" select="2"/>
                <field name="estado_id" select="0"/>

            </group>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="view_municipio_tree">
        <field name="name">municipio.tree</field>
        <field name="model">municipio</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="municipio">
                <field name="codigo"/>
                <field name="municipio"/>
                <field name="estado_id"/>

            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="action_municipio">
        <field name="name">municipio</field>
        <field name="res_model">municipio</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>

        
    <record model="ir.ui.view" id="view_parroquia_form">
        <field name="name">parroquia.form</field>
        <field name="model">parroquia</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="parroquia">
            <group col="4" colspan="2">
                <field name="codigo" select="1"/>
                <field name="parroquia" select="2"/>
                <field name="municipio_id" select="0"/>

            </group>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="view_parroquia_tree">
        <field name="name">parroquia.tree</field>
        <field name="model">parroquia</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="parroquia">
                <field name="codigo"/>
                <field name="parroquia"/>
                <field name="municipio_id"/>

            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="action_parroquia">
        <field name="name">parroquia</field>
        <field name="res_model">parroquia</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>
    
        
    <record model="ir.ui.view" id="view_proyectostrabajados_form">
        <field name="name">proyectostrabajados.form</field>
        <field name="model">proyectostrabajados</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="proyectostrabajados">
            <group col="4" colspan="2">
                <field name="proyecto" select="1"/>
                

            </group>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="view_proyectostrabajados_tree">
        <field name="name">proyectostrabajados.tree</field>
        <field name="model">proyectostrabajados</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="proyectostrabajados">
                <field name="proyecto"/>
                <field name="activista_id"/>

            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="action_proyectostrabajados">
        <field name="name">Proyectos Trabajado</field>
        <field name="res_model">proyectostrabajados</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>
	
	<!-- Registro de Activistas -->
	<menuitem name="Registro de Activistas" id="menu_registro"/>
    <menuitem name="Registro de Activistas/Formulario/Activista" id="menu_activista" action="action_activista"/>
	
	<!-- Configuracion -->
	<menuitem name="Configuración" id="menu_configuracion" parent="menu_registro"/>
	
	<!-- Sub-Menu de configuracion -->
    <menuitem name="Telefono Del Activista" id="menu_telefonoactivista" action="action_telefonoactivista" parent="menu_configuracion"/>
    <menuitem name="Estado" id="menu_estado" action="action_estado" parent="menu_configuracion"/>
    <menuitem name="municipio" id="menu_municipio" action="action_municipio" parent="menu_configuracion"/>
    <menuitem name="parroquia" id="menu_parroquia" action="action_parroquia" parent="menu_configuracion"/>
    <menuitem name="Proyectos Trabajado" id="menu_proyectostrabajados" action="action_proyectostrabajados" parent="menu_configuracion"/>

        
</data>
</openerp>
